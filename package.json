  },
  "files": [
    "dist/**/*.js*",
    "types/**/*.d.ts"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/react-loading-progress-indicator.public.d.ts",
  "sideEffects": false,
  "scripts": {
    "api-extractor:check": "api-extractor run --verbose",
