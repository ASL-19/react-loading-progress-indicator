    "eslint-plugin-tsdoc": "^0.2.5",
    "eslint-plugin-unicorn": "^18.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.3"
